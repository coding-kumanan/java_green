package stringexamples.org;


public class LengthOfTheString {
	public int stringLength(String string) {
		int length =string.length();
		return length;
		
	}
	public static void main (String[] args) {
		LengthOfTheString lengthString = new LengthOfTheString();
		 System.out.println("The length of the given string is" +lengthString.stringLength("kumanan"));
	}
}
