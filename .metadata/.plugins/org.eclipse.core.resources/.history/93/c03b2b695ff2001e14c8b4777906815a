package stringexamplesInheritance.org;

public class Mainclass extends DataClass {

	public static void main(String[] args) {
		Mainclass output = new Mainclass();
		System.out.println(output.findChar(output.kumanan, 1));
		System.out.println(output.findChar(output.mohit, 3));
		System.out.println(output.findIndex(output.ph, '9'));
		
		String uLlU = "WeLcOmE";
		String x = "";
		for(int i=0;i<uLlU.length();i++) {
			//char ch = uLlU.charAt(i);
			if(Character.isLowerCase(uLlU.charAt(i)))
			{
				 x+= Character.toUpperCase(uLlU.charAt(i));
			}
			else {
				x+=Character.toLowerCase(uLlU.charAt(i));
			}
		}
		System.out.println(x);
	}

}
